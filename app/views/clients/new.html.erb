<img src="/images/242x82.png"> </img>


<div class="signup_block">

  <%= form_for @client do |f| %>


      <div class="section_block" id="section_block">
        <div class="login_content">
          <div class="login-head">
            <h1>clients Page</h1>
          </div><!--login-head-->




          <% if @client.errors.any? %>
              <div class="error_messages">
                <h2>client is invalid</h2>
                <ul>
                  <% for message in @client.errors.full_messages %>
                      <li><%= message %></li>
                  <% end %>
                </ul>
              </div>
          <% end %>
          <div class="form-group">
            <%= f.label :name %>
            <%= f.text_field :name %>
          </div>
          <div class="form-group">
            <%= f.label :site %>
            <%= f.url_field :site %>
          </div>

          <div class="button"><%= f.submit %></div>
        </div>
      </div>
  <% end %>
</div>
